
FUNCTION mmul :
PARAM c 
PARAM b 
t2(2) := c * #2 
t1(1) := t2(2) - b 
RETURN t1(1) 

FUNCTION main :
READ n 
READ mul 
t3(3) := #40 
tt := t3(3) * #7 
ARG n 
t4(4) := tt * mul 
ARG t4(4) 
v := CALL mmul 
WRITE mul 
WRITE tt 
WRITE v 
LABEL label1(0) :
IF n >(0) #1 GOTO label2(0) 
GOTO label3(0) 
LABEL label2(0) :
READ t5(5) 
a := t5(5) 
t6(6) := a * mul 
res := t6(6) 
WRITE res 
t7(7) := n - #1 
n := t7(7) 
WRITE n 
GOTO label1(0) 
LABEL label3(0) :
RETURN #0 
