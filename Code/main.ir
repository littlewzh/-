
FUNCTION main :
READ n 
READ mul 
LABEL label1 :
t1 := #0 
IF n > #0 GOTO label4 
GOTO label5 
LABEL label4 :
t1 := #1 
LABEL label5 :
IF t1 != #0 GOTO label2 
GOTO label3 
LABEL label2 :
READ t2 
a := t2 
t3 := a * mul 
res := t3 
WRITE res 
t4 := n - #1 
n := t4 
GOTO label1 
LABEL label3 :
RETURN #0 
