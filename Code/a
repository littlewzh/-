
FUNCTION foo :
PARAM sx 
DEC s22 128 
bar := #1 
t4 := &s22 
t5 := t4 + #8 
t3 := t5 
t2 := t3 + #48 
t1 := t2 + #12 
t6 := #1 
*t1 := t6 

FUNCTION main :
DEC a 24 
t8 := &a + #12 
t7 := t8 + #4 
t9 := #1 
*t7 := t9 
t11 := &a + #12 
t10 := t11 + #8 
b := *t10 
t13 := &a + #0 
t12 := t13 + #4 
t14 := b 
*t12 := t14 
